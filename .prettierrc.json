{
	"semi": true,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 80,
	"tabWidth": 2,
	"useTabs": true,
	"bracketSpacing": true,
	"importOrder": [
		"^react$",
		"^react-dom$",
		"<THIRD_PARTY_MODULES>",
		"^@/(.*)$",
		"^[./]"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"plugins": ["@trivago/prettier-plugin-sort-imports"]
}
